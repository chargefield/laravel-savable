{
    "name": "chargefield/supermodels",
    "description": "A Laravel package that will turn your models into supermodels.",
    "keywords": [
        "chargefield",
        "supermodels"
    ],
    "homepage": "https://github.com/chargefield/supermodels",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Clayton D'Mello",
            "email": "clayton@chargefield.com",
            "homepage": "https://chargefield.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chargefield\\Supermodels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chargefield\\Supermodels\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chargefield\\Supermodels\\SupermodelsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "php_cs_fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

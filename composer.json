{
    "name": "chargefield/supermodel",
    "description": "A Laravel package that will turn your models into supermodels.",
    "keywords": [
        "chargefield",
        "supermodel"
    ],
    "homepage": "https://github.com/chargefield/supermodel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Clayton D'Mello",
            "email": "clayton@chargefield.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chargefield\\Supermodel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chargefield\\Supermodel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}

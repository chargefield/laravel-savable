{
    "name": "chargefield/laravel-savable",
    "description": "Savable is a Laravel package that will help you organize your business logic.",
    "keywords": [
        "chargefield",
        "laravel",
        "savable"
    ],
    "homepage": "https://github.com/chargefield/laravel-savable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Clayton D'Mello",
            "email": "clayton@chargefield.com",
            "homepage": "https://chargefield.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chargefield\\Savable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chargefield\\Savable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "SavableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "php_cs_fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
